type Query {
    userByEmail(email: ID!): User
}

type Mutation {

}

type User {
    email: ID!
    username: String!
    address: Address!
    friends: [String]
    articles: [Article]
    sharedArticles: [Article]
}

type Address {
    postalCode: String
    street: String
    streetNumber: String
    complement: String
}

type Article {
    id: String!
    url: String!
    tags: [String]
    suggestedTags: [String]
}